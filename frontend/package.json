    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "@trendmicro/react-sidenav": "^0.5.0",
    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "cypress": "^9.5.1",
    "enzyme": "^3.11.0",
    "jest-html-reporter": "^3.4.2",
